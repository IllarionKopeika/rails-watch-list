<div class="container">
  <div class="justify-content-center row">
    <div class="col-sm-8 col-sm-offset-2">
      <h1>New list</h1>
        <%= simple_form_for(@list) do |f| %>
        <div>
          <%= f.input :name %>
        </div>
        <div>
          <%= f.button :submit, class: "btn btn-outline-secondary" %>
          <%= link_to "Back", lists_path, class: "btn btn-outline-secondary" %>
        </div>
        <% end %>
    </div>
  </div>
</div>
